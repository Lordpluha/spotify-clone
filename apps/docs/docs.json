{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Spotify-clone",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "General",
            "pages": [
              "general/ARCHITECTURE",
              "general/CODE_OF_CONDUCT",
              "general/CONTRIBUTING",
              "general/STYLE_GUIDE"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/webhook",
              "api-reference/get",
              "api-reference/delete"
            ]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "@spotify/web - develop",
        "href": "https://spotify-clone-web-git-develop-vladyslavs-projects-cc52700b.vercel.app/"
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/lordpluha",
      "linkedin": "https://www.linkedin.com/in/vladyslavtesliuk/"
    }
  },
  "plugins": [
    "@theguild/openapi-mdx"
  ],
  "openapi": {
    "spec": "./api-reference/openapi.json"
  },
  "mdxPlugins": [
    "remark-toc",
    "remark-code-titles",
    "rehype-slug",
    "rehype-autolink-headings"
  ]
}