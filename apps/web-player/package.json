{
  "name": "@spotify/web-player",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "predev": "pnpm --filter @spotify/ui-react run build",
    "dev": "next dev --port 3001",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:debug": "rm -rf .next && rm -rf node_modules/.cache && DEBUG=\"next:*,webpack:*,turbo:*\" NODE_OPTIONS=\"--max_old_space_size=8192 --trace-warnings\" npm run build",
    "start": "next start",
    "lint": "pnpm exec biome lint --write",
    "check-types": "tsc --noEmit",
    "format": "pnpm exec biome format --write"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@reduxjs/toolkit": "^2.11.2",
    "@spotify/ui-react": "workspace:*",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/react-transition-group": "^4.4.12",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "motion": "^12.33.0",
    "next": "^16.1.3",
    "openapi-fetch": "^0.15.0",
    "openapi-react-query": "^0.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-redux": "^9.2.0",
    "react-transition-group": "^4.4.5",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@spotify/contracts": "workspace:*",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.2",
    "@types/react": "~19.1.17",
    "@types/react-dom": "~19.1.11",
    "autoprefixer": "^10.4.23",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
