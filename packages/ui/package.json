{
  "name": "@spotify/ui",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./components/*": {
      "types": "./dist/types/components/*.d.ts",
      "import": "./dist/esm/components/*.js",
      "require": "./dist/cjs/components/*.js"
    },
    "./hooks/*": {
      "types": "./dist/types/hooks/*.d.ts",
      "import": "./dist/esm/hooks/*.js",
      "require": "./dist/cjs/hooks/*.js"
    },
    "./lib/*": {
      "types": "./dist/types/lib/*.d.ts",
      "import": "./dist/esm/lib/*.js",
      "require": "./dist/cjs/lib/*.js"
    },
    "./globals.css": "./dist/esm/globals.css",
    "./postcss.config": "./postcss.config.cjs",
    "./tailwind.preset": {
      "types": "./tailwind.preset.d.ts",
      "default": "./tailwind.preset.cjs"
    }
  },
  "files": [
    "dist",
    "postcss.config.cjs",
    "tailwind.preset.cjs",
    "tailwind.preset.d.ts"
  ],
  "scripts": {
    "lint": "eslint src --max-warnings 0",
    "check-types": "tsc --noEmit",
    "prettier": "prettier \"./src/**/*.{ts,tsx,json,js,scss,css}\" --write --list-different --cache",
    "stylelint": "stylelint \"./src/**/*.{scss,css}\" --config @spotify/stylelint --fix --cache --cache-location node_modules/.cache/stylelint",
    "format": "npm run prettier && npm run stylelint",
    "storybook": "storybook dev -p 6006",
    "clean": "rimraf dist .types",
    "build-storybook": "storybook build",
    "build:js": "rollup -c rollup.config.mjs",
    "build:types:prep": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:types": "rollup -c rollup.config.dts.mjs",
    "build": "pnpm clean && pnpm build:js && pnpm build:types:prep && pnpm build:types",
    "dev": "rollup -c rollup.config.mjs -w",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "prettier": "@spotify/prettier",
  "peerDependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@spotify/eslint": "workspace:*",
    "@spotify/prettier": "workspace:*",
    "@spotify/stylelint": "workspace:*",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "chromatic": "^13.1.4",
    "eslint": "^9.34.0",
    "eslint-plugin-storybook": "^9.1.3",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.2.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^9.1.17",
    "stylelint": "^16.23.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "react-hook-form": "^7.61.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.7",
    "zod": "^4.1.5"
  }
}
