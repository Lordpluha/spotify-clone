{
  "name": "@spotify/ui",
  "version": "0.0.1",
  "private": true,
  "exports": {
    "./globals.css": "./src/globals.css",
    "./postcss.config": "./postcss.config.mjs",
    "./tailwind.config": "./tailwind.config.ts",
    "./lib/*": "./src/lib/*.ts",
    "./components/*": [
      "./src/components/*.tsx",
      "./src/components/*.ts"
    ],
    "./hooks/*": "./src/hooks/*.ts",
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --max-warnings 0",
    "check-types": "tsc --noEmit",
    "prettier": "prettier \"./src/**/*.{ts,tsx,json,js,scss,css}\" --write --list-different --cache",
    "stylelint": "stylelint \"./src/**/*.{scss,css}\" --config @spotify/stylelint --fix --cache --cache-location node_modules/.cache/stylelint",
    "format": "npm run prettier && npm run stylelint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prettier": "@spotify/prettier",
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@spotify/eslint": "workspace:*",
    "@spotify/prettier": "workspace:*",
    "@spotify/stylelint": "workspace:*",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/parser": "^7.18.0",
    "chromatic": "^13.1.3",
    "eslint": "^9.33.0",
    "eslint-plugin-storybook": "^9.1.2",
    "storybook": "^9.1.2",
    "stylelint": "^16.23.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "add": "^2.0.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.540.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.7",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.17"
  }
}
