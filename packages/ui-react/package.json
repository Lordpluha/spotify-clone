{
  "name": "@spotify/ui-react",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./globals.css": {
      "style": "./dist/globals.css",
      "default": "./dist/globals.css"
    },
    "./src": {
      "default": "./src"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "pnpm exec biome lint --write",
    "format": "pnpm exec biome format --write",
    "storybook": "storybook dev -p 6006",
    "clean-build": "rimraf dist",
    "clean-svgr": "rimraf src/icons/svgr",
    "build-storybook": "storybook build",
    "gen:tokens": "tokens-generator --tokens ../tokens/tokens.json --config ./tokens.config.mjs --output ./src/styles",
    "svgr:build": "react-svgr build -i @spotify/tokens/icons -o src/icons/svgr --variables \"primaryColor,secondaryColor\"",
    "svgr:dev": "react-svgr dev -i @spotify/tokens/icons -o src/icons/svgr --variables \"primaryColor,secondaryColor\"",
    "build": "pnpm clean-build && pnpm svgr:build && react-bundler build",
    "dev": "pnpm clean-build && react-bundler dev",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@spotify/esbuild-bundler": "workspace:*",
    "@spotify/svgr": "workspace:*",
    "@spotify/tokens": "workspace:*",
    "@spotify/tokens-generator": "workspace:*",
    "@storybook/addon-a11y": "^10.2.7",
    "@storybook/addon-docs": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^25.2.2",
    "@types/react": "~19.1.17",
    "@types/react-dom": "~19.1.11",
    "chromatic": "^15.0.0",
    "esbuild": "0.27.3",
    "glob": "^13.0.0",
    "rimraf": "^6.1.2",
    "storybook": "^10.2.7",
    "tailwindcss": "^4.1.18",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color-convert": "^3.1.3",
    "color-name": "^2.1.0",
    "lucide-react": "^0.563.0",
    "react-hook-form": "^7.71.1",
    "react-resizable-panels": "^3.0.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.4"
  }
}
