{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "tags": ["-lintignore"],
  "workspaces": {
    ".": {
      "entry": [".cz-config.js"],
      "ignore": [".cz-config.js"],
      "ignoreBinaries": ["biome"]
    },
    "apps/api": {
      "entry": ["src/main.ts", "src/**/*.module.ts", "src/infra/**/*.ts"],
      "ignoreFiles": ["prisma.config.ts"],
      "prisma": false,
      "ignore": ["src/**/index.ts"]
    },
    "apps/web-artists": {
      "entry": [
        "src/app/**/*.tsx",
        "src/app/**/*.ts",
        "src/app/global.css"
      ],
      "ignore": ["env.schema.ts"]
    },
    "apps/web-player": {
      "entry": [
        "src/app/**/*.tsx",
        "src/app/**/*.ts",
        "src/app/global.css"
      ],
      "ignore": ["env.schema.ts"]
    },
    "packages/ui-react": {
      "entry": ["src/**/index.css"],
      "ignoreDependencies": ["chromatic", "tsc-alias"]
    }
  },
  "ignore": [
    "**/*.module.css",
    "**/*.stories.tsx",
    "**/*.test.tsx",
    "**/dist/**",
    "**/.next/**",
    "**/types/**/*.types.ts",
    "**/node_modules/**"
  ],
  "rules": {}
}
